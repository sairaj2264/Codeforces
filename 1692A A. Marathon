#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	
	int test;
	cin>>test;
	
	while(test-- > 0){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    int arr[4];
	    int n = 4;
	    arr[0] = a;
	    arr[1] = b;
	    arr[2] = c;
	    arr[3] = d;
	    
	    sort(arr , arr + n);
	    
	    if(arr[0] == a){
	        cout<<3<<endl;
	    }else if(arr[1] == a){
	        cout<<2<<endl;
	    }else if(arr[2] == a){
	        cout<<1<<endl;
	    }else{
	        cout<<0<<endl;
	    }
	    
	    
	}

}
