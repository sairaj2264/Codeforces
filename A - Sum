#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	
	int test;
	cin>>test;
	
	for(int t=0;t<test;t++){
	    int a,b,c;
	    cin >>a>>b>>c;
	    
	    
	    if((a+b) == c){
	        cout<<"YES"<<endl;
	    }else if((a+c) == b){
	        cout<<"YES"<<endl;
	    }else if((b+c) == a){
	        cout<<"YES"<<endl;
	    }else{
	        cout<<"NO"<<endl;
	    }
	}
	
 
	    
	    
	}
 
